{
  "name": "react16-babel7-webpack4",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --print-width 120 --write"
    ],
    "*.less": [
      "stylelint --fix",
      "prettier --parser less --print-width 120 --write"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "cross-env NODE_ENV=development webpack-serve --open",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "anywhere -d ./build/ -f -l",
    "stylelint": "stylelint --fix",
    "test": "cross-env NODE_ENV=test jest --env=jsdom --notify --watchman=false",
    "coverage": "cross-env NODE_ENV=test jest --env=jsdom -u --coverage"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "@": "./src"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "react-hot-loader/babel",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-modules-commonjs",
          [
            "module-resolver",
            {
              "root": [
                "."
              ],
              "alias": {
                "@": "./src"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "testMatch": [
      "<rootDir>/__tests__/**/*.js?(x)",
      "<rootDir>/__tests__/**/*.ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|scss|less)$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-prettier",
      "stylelint-config-standard"
    ],
    "rules": {
      "indentation": 2,
      "string-quotes": "single"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/history": "^4.7.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "antd": "^3.9.2",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-relay": "^0.0.28",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "immutability-helper-x": "^1.0.5",
    "internal-ip": "^3.0.1",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "koa-connect": "^2.0.1",
    "koa-router": "^7.4.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.2",
    "object-assign": "^4.1.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prettier-stylelint": "^0.4.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "promise": "^8.0.2",
    "react-test-renderer": "^16.5.1",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.6",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.2",
    "webpack-serve-overlay": "^0.3.0",
    "webpack-serve-waitpage": "^1.0.2",
    "whatwg-fetch": "^3.0.0"
  }
}
